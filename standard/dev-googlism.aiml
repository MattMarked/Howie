<!--
This file includes support for "who/what/where/when is" questions,
through www.googlism.com.
-->

<aiml version="1.0">

<category>
<pattern>WHO IS *</pattern>
<template>
<system mode="sync">scripts\googlism.py "<input/>"</system>
</template>
</category>

<category>
<pattern>WHAT IS *</pattern>
<template>
<srai>WHO IS <star/></srai>
</template>
</category>

<category>
<pattern>WHERE IS *</pattern>
<template>
<srai>WHO IS <star/></srai>
</template>
</category>

<category>
<pattern>WHEN IS *</pattern>
<template>
<srai>WHO IS <star/></srai>
</template>
</category>


<category>
<pattern>WHO WAS *</pattern>
<template>
<srai>WHO IS <star/></srai>
</template>
</category>

